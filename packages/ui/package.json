{
  "name": "accelerate-cms-ui",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "sb": "storybook dev -p 6006",
    "dev": "npm run build -- --watch",
    "build": "npm run clean && tsup src/index.tsx --format esm,cjs --dts --external react",
    "clean": "rimraf dist storybook-static"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@radix-ui/react-icons": "^1.1.1",
    "@storybook/addon-actions": "^7.0.0-beta.45",
    "@storybook/addon-essentials": "^7.0.0-beta.45",
    "@storybook/addon-interactions": "^7.0.0-beta.45",
    "@storybook/addon-links": "^7.0.0-beta.45",
    "@storybook/addons": "^7.0.0-beta.45",
    "@storybook/react": "^7.0.0-beta.45",
    "@storybook/react-vite": "^7.0.0-beta.45",
    "@storybook/testing-library": "^0.0.14-next.1",
    "@storybook/theming": "^7.0.0-beta.45",
    "@tailwindcss/forms": "^0.5.3",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@vitejs/plugin-react": "^3.1.0",
    "autoprefixer": "^10.4.13",
    "classnames": "^2.3.2",
    "eslint": "^8.33.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^0.6.10",
    "postcss": "^8.4.21",
    "prettier": "latest",
    "storybook": "^7.0.0-beta.45",
    "tailwindcss": "^3.2.6",
    "tsup": "^6.6.3",
    "typescript": "^4.9.3",
    "vite": "^4.1.0",
    "vite-plugin-dts": "^1.7.2"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./package.json": "./package.json",
    "./dist/index.css": "./dist/index.css"
  }
}
