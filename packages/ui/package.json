{
  "name": "accelerate-cms-ui",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "sb": "storybook dev -p 6006",
    "dev": "pnpm run build -- --watch",
    "build": "pnpm run clean && tsup src/index.tsx --format esm,cjs --dts --external react",
    "clean": "rimraf dist storybook-static",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@tanstack/react-table": "^8.7.9",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@radix-ui/react-icons": "^1.2.0",
    "@storybook/addon-actions": "^7.0.0-rc.4",
    "@storybook/addon-essentials": "^7.0.0-rc.4",
    "@storybook/addon-interactions": "^7.0.0-rc.4",
    "@storybook/addon-links": "^7.0.0-rc.4",
    "@storybook/addons": "^7.0.0-rc.4",
    "@storybook/react": "^7.0.0-rc.4",
    "@storybook/react-vite": "^7.0.0-rc.4",
    "@storybook/testing-library": "^0.0.14-next.1",
    "@storybook/theming": "^7.0.0-rc.4",
    "@tailwindcss/forms": "^0.5.3",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@vitejs/plugin-react": "^3.1.0",
    "autoprefixer": "^10.4.14",
    "classnames": "^2.3.2",
    "eslint": "^8.36.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^0.6.11",
    "postcss": "^8.4.21",
    "prettier": "latest",
    "storybook": "^7.0.0-rc.4",
    "tailwindcss": "^3.2.7",
    "tsup": "^6.6.3",
    "typescript": "^4.9.5",
    "vite": "^4.2.0",
    "vite-plugin-dts": "^1.7.3"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./package.json": "./package.json",
    "./dist/index.css": "./dist/index.css",
    "./tailwind.config": "./tailwind.config.cjs"
  }
}
