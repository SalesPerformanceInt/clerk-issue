{
  "name": "accelerate_cms",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "turbo run dev",
    "sb": "turbo run sb --scope=accelerate-cms-ui",
    "build": "turbo run build",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^3.7.1",
    "@types/eslint": "^8.21.0",
    "@typescript-eslint/eslint-plugin": "^5.51.0",
    "@typescript-eslint/parser": "^5.51.0",
    "concurrently": "^7.6.0",
    "eslint": "^8.33.0",
    "eslint-config-prettier": "^8.6.0",
    "prettier": "latest",
    "prettier-plugin-tailwindcss": "^0.2.2",
    "rimraf": "^4.1.2",
    "turbo": "latest",
    "typescript": "^4.9.3"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
